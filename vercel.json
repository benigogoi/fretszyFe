{
    "buildCommand": "node install-binaries.js && npm run build",
    "installCommand": "npm install --platform=linux --arch=x64 --force",
    "framework": "vite",
    "redirects": [
      {
        "source": "/:path*",
        "has": [
          {
            "type": "host",
            "value": "fretszy.com"
          }
        ],
        "destination": "https://www.fretszy.com/:path*",
        "permanent": true
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  